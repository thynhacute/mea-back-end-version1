    "scripts": {
        "prebuild": "rimraf dist",
        "build:dev": "NODE_ENV=development nest build",
        "build:prod": "NODE_ENV=development nest build",
        "format": "prettier --write \"src/**/*.ts\" ",
        "start": "NODE_ENV=development DEBUG=app-*,http-* nest start",
        "start:migrate": "NODE_ENV=migration DEBUG=app-*,http-* nest start",
        "start:reset": "NODE_ENV=reset DEBUG=app-*,http-* nest start",
        "start:dev": "NODE_ENV=development DEBUG=app-*,http-* nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "NODE_ENV=development DEBUG=app-*,http-* node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "NODE_ENV=test jest --runInBand  --forceExit --silent=false",
        "test:watch": "NODE_ENV=test  jest --watch --runInBand ",
        "test:cov": "NODE_ENV=test  jest --coverage --runInBand  --forceExit",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "NODE_ENV=testing jest --config ./test/jest-e2e.json",
        "init:db": "bash ./scripts/db.sh",
        "prepare": "husky install",
        "commit": "cz"
    },
